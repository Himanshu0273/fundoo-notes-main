{
  "handlers": [
    {
      "sink": "user_logs/config_log.log",
      "format": "{level:<6} | {time:YYYY-MM-DD HH:mm:ss} | {name:<20} | {line} | {message}",
      "rotation": "1 MB",
      "compression": "zip",
      "backtrace": false,
      "diagnose": false,
      "level": "INFO",
      "filter": "config"
    },
    {
      "sink": "user_logs/func_log.log",
      "format": "{level:<6} | {time:YYYY-MM-DD HH:mm:ss} | {name:<20} | {line} | {message}",
      "rotation": "1 MB",
      "compression": "zip",
      "backtrace": false,
      "diagnose": false,
      "level": "INFO",
      "filter": "func"
    },
    {
      "sink": "notes_logs/func_log.log",
      "format": "{level:<6} | {time:YYYY-MM-DD HH:mm:ss} | {name:<20} | {line} | {message}",
      "rotation": "1 MB",
      "compression": "zip",
      "backtrace": false,
      "diagnose": false,
      "level": "INFO",
      "filter": "func1"
    }
  ]
}